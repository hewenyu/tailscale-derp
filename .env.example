TAILSCALE_DERP_HOSTNAME=derp.example.com
TAILSCALE_DERP_ADDR=:1443
TAILSCALE_DERP_VERIFY_CLIENTS=true
TAILSCALE_DERP_STUN_PORT=3478
TAILSCALE_AUTH_KEY=tskey-auth-**************
# 1panel证书文件路径 (在容器内的路径)
TAILSCALE_DERP_CERT_FILE=/certs/fullchain.pem
TAILSCALE_DERP_KEY_FILE=/certs/privkey.pem

# 是否在容器内运行 tailscaled（默认 true）。
# 如要复用宿主机 tailscaled，请设置为 false，并在 docker-compose 中挂载宿主机 socket：
#   - /var/run/tailscale/tailscaled.sock:/var/run/tailscale/tailscaled.sock:ro
# 宿主机模式下可删除 cap_add 和 /dev/net/tun 挂载，且无需 TAILSCALE_AUTH_KEY。
TAILSCALE_EMBEDDED_TAILSCALED=true
